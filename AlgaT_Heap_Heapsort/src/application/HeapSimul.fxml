<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.HeapSimulController">
   <children>
      <Pane fx:id="vectorPane" layoutX="19.0" layoutY="60.0" prefHeight="134.0" prefWidth="350.0">
         <children>
            <HBox fx:id="vectorHbox" layoutX="14.0" layoutY="14.0" prefHeight="100.0" prefWidth="310.0" />
         </children>
      </Pane>
      <Pane fx:id="treePane" layoutX="386.0" layoutY="60.0" prefHeight="440.0" prefWidth="400.0">
         <children>
            <HBox fx:id="treeLevel0" layoutX="170.0" layoutY="14.0" prefHeight="100.0" prefWidth="42.0" />
            <HBox fx:id="treeLevel1" layoutX="70.0" layoutY="114.0" prefHeight="100.0" prefWidth="250.0" spacing="170.0" />
            <HBox fx:id="treeLevel2" layoutX="25.0" layoutY="214.0" prefHeight="100.0" prefWidth="350.0" spacing="62.0" />
            <HBox fx:id="treeLevel3" layoutY="314.0" prefHeight="100.0" prefWidth="400.0" spacing="10.0" />
         </children>
      </Pane>
      <HBox layoutX="26.0" layoutY="194.0" prefHeight="50.0" prefWidth="360.0" spacing="30.0">
         <children>
            <TextField fx:id="inputArea" prefHeight="35.0" prefWidth="70.0" />
            <Button fx:id="addButton" mnemonicParsing="false" onAction="#addToVector" prefHeight="35.0" prefWidth="50.0" text="Add" />
            <Button fx:id="removeButton" mnemonicParsing="false" onAction="#removeFromVector" prefHeight="35.0" prefWidth="60.0" text="Remove" />
            <Button fx:id="generateButton" mnemonicParsing="false" onAction="#generateHeap" prefHeight="35.0" prefWidth="80.0" text="Generate" />
         </children>
      </HBox>
      <ChoiceBox fx:id="maxMinChoiceBox" layoutX="289.0" layoutY="244.0" prefHeight="25.0" prefWidth="80.0" />
   </children>
</AnchorPane>
